class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int n=nums.size();
        int i=1, count=1;
        for(int j=1; j<n; j++){
            if(nums[j]!=nums[j-1]) {
                nums[i]=nums[j];
                i++;
                count++;
            }
        }
        return count;
    }
    
};
